<xml>
    <ctbk_version>14</ctbk_version>
    <wifi_tx_pwr>
        <Default_5GHZ   index="0">16</Default_5GHZ> <!--5G, Max power-->
        <State1_5GHZ    index="1">16</State1_5GHZ>  <!--5G,WLAN+WWAN-->
        <State2_5GHZ    index="2">16</State2_5GHZ>  <!--5G,MHS, WLAN+WAN-->
        <Default_2GHZ   index="3">19</Default_2GHZ> <!--2G, Max power-->
        <State1_2GHZ    index="4">19</State1_2GHZ> <!--2G,WLAN+WWAN-->
        <State2_2GHZ    index="5">19</State2_2GHZ> <!--2G,MHS, WLAN+WAN-->
        <State4_5GHZ    index="6">16</State4_5GHZ>  <!--5G,At head, WLAN only-->
        <State5_5GHZ    index="7">16</State5_5GHZ>  <!--5G,At head, WLAN+WWAN-->
        <State6_5GHZ    index="8">13</State6_5GHZ>  <!--5G,Body-Worn, WLAN only-->
        <State7_5GHZ    index="9">13</State7_5GHZ>  <!--5G,Body-Worn, WLAN+WWAN-->
        <State4_2GHZ    index="10">19</State4_2GHZ>  <!--2G,At head, WLAN only-->
        <State5_2GHZ    index="11">19</State5_2GHZ>  <!--2G,At head, WLAN+WWAN-->
        <State6_2GHZ    index="12">17</State6_2GHZ>  <!--2G,Body-Worn, WLAN only-->
        <State7_2GHZ    index="13">17</State7_2GHZ>  <!--2G,Body-Worn, WLAN+WWAN-->
    </wifi_tx_pwr>
    <sar_sensors>
        <sensor type="prox_front"           index="0">EPL259x ALS/PS PROX</sensor>
        <sensor type="prox_back"            index="1">null</sensor>
        <sensor type="cap_bottom_center"    index="2">capsense_bottom</sensor>
        <sensor type="cap_bottom_right"     index="3">null</sensor>
        <sensor type="cap_bottom_left"      index="4">null</sensor>
        <sensor type="cap_top"              index="5">capsense_top</sensor>
    </sar_sensors>
    <!--OB5 config: 9 = 00001001b (cap_bottom_center|cap_top)-->
    <ob5_sensor_config>9</ob5_sensor_config>
</xml>
